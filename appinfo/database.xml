<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
    <name>*dbname*</name>
    <create>true</create>
    <overwrite>false</overwrite>
    <charset>utf8</charset>
    <table>
        <name>*dbprefix*imgcrawl_urlcache</name>
        <declaration>
            <field>
                <name>id</name>
                <type>integer</type>
                <default>0</default>
                <notnull>true</notnull>
                <autoincrement>1</autoincrement>
            </field>
            <field>
                <name>url</name>
                <type>text</type>
                <notnull>true</notnull>
                <length>512</length>
                <comments>Url of the image (given by rss feed post)</comments>
            </field>
            <field>
                <name>created</name>
                <type>timestamp</type>
                <notnull>true</notnull>
            </field>
        </declaration>
    </table>
</database>
